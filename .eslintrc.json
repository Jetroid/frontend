{
  "extends": [
    "google",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["flowtype", "react", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "react/prop-types": "off",
    "camelcase": "off",
    "prettier/prettier": "warn"
  }
}
